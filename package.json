{
	"name": "dapp-template",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"build": "truffle compile --all && onlyBuild=true truffle deploy --network development && truffle compile",
		"console": "truffle console",
		"deploy": "truffle deploy --network development",
		"rinkeby": "truffle deploy --network rinkeby",
		"matic": "truffle deploy --network matic",
		"goerli": "truffle deploy --network goerli",
		"test": "useCache=true truffle test --network development --compile-none",
		"coverage": "truffle run coverage --network development",
		"prepare-docs": "scripts/prepare-docs.sh",
		"prettier:solidity": "prettier --write 'contracts/**/*.sol'",
		"lint": "prettier --write contracts/**/*.sol && solhint -f unix contracts/**/*.sol"
	},
	"keywords": [],
